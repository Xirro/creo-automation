<body>

<div id="wrapper">
    <%- include('../partials/header') %>

    <%- include('../partials/sidebar') %>

    <div class="content-page">
        <!-- Content Start -->

        <div class="content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="page-title-box">
                            <h4 class="page-title">Current Layout - Latest Rev</h4>
                            <div class="row justify-content-center">
                                <div class="col-sm-6">
                                    <div class="card m-b-20" style="height:94%;">
                                        <div class="card-body">
                                            <h4 class="mt-0 header-title">Project Data</h4>
                                            <% if (message != null) { %>
                                                <% if (message.location == 1) { %>
                                                    <h6 style="color:red;"><%= message.text %></h6>
                                                <% } %>
                                            <% } %>
                                            <% if  (layoutData.length != 0) { %>
                                                <% for (let i = 0; i < layoutData.length; i++) { %>
                                                <form id="layoutForm" class="layoutForm" action="" method="POST">
                                                    <div class="form-group">
                                                        <div class="row">
                                                            <div class="col-sm-3">
                                                                <label for="example-text-input" class="col-form-label">Job #</label>
                                                                <input type="text" name="jobNum" class="form-control" value="<%=layoutData[i].jobNum%>" placeholder="Type Number Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                            <div class="col-sm-2">
                                                                <label for="example-text-input" class="col-form-label">Release</label>
                                                                <input type="text" name="releaseNum" class="form-control" value="<%=layoutData[i].releaseNum%>" placeholder="Type Release Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                            <div class="col-sm-2">
                                                                <label for="example-text-input" class="col-form-label">Rev</label>
                                                                <input type="text" name="revNum" class="form-control" value="00" placeholder="Type Rev Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                            <div class="col-sm-5">
                                                                <label for="example-text-input" class="col-form-label">Customer</label>
                                                                <input type="text" name="customer" class="form-control" value="<%=layoutData[i].customer%>" placeholder="Type Customer Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                        </div>
                                                        <div class="row">
                                                            <div class="col-sm-12">
                                                                <label for="example-text-input" class="col-form-label">Job Name</label>
                                                                <input type="text" name="jobName" class="form-control" value="<%=layoutData[i].jobName%>" placeholder="Type Name Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                        </div>
                                                        <div class="row">
                                                            <div class="col-sm-12">
                                                                <label for="example-text-input" class="col-form-label">Layout Name</label>
