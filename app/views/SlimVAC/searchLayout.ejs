                                                                            <div class="form-group">
                                                                                <p class="mb-0">
                                                                                    Please fill out the following and click "Save" afterwards
                                                                                </p>
                                                                                <p class="text-muted m-b-5"></p>

                                                                                <div class="row">
                                                                                    <div class="col-sm-2">
                                                                                        <label for="example-text-input" class="col-form-label">Job #</label>
                                                                                        <input type="text" name="jobNum" class="form-control" value="<%=layoutData[i].jobNum%>" placeholder="Type Number Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                                                    </div>
                                                                                    <div class="col-sm-2">
                                                                                        <label for="example-text-input" class="col-form-label">Release</label>
                                                                                        <input type="text" name="releaseNum" class="form-control" value="<%=layoutData[i].releaseNum%>" placeholder="Type Release Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                                                    </div>
                                                                                    <div class="col-sm-2">
                                                                                        <label for="example-text-input" class="col-form-label">Rev</label>
                                                                                        <input type="text" name="revNum" class="form-control" value="00" placeholder="Type Rev Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                                                    </div>
                                                                                    <div class="col-sm-6">
                                                                                        <label for="example-text-input" class="col-form-label">Customer</label>
                                                                                        <input type="text" name="customer" class="form-control" value="<%=layoutData[i].customer%>" placeholder="Type Customer Here" id="example-text-input" autocomplete="off">
                                                                                    </div>
                                                                                </div>
                                                                                <div class="row">
                                                                                    <div class="col-sm-12">
                                                                                        <label for="example-text-input" class="col-form-label">Job Name</label>
                                                                                        <input type="text" name="jobName" class="form-control" value="<%=layoutData[i].jobName%>" placeholder="Type Name Here" id="example-text-input" autocomplete="off">
                                                                                    </div>
                                                                                </div>
                                                                                <div class="row">
                                                                                    <div class="col-sm-12">
                                                                                        <label for="example-text-input" class="col-form-label">Layout Name</label>
                                                                                        <input type="text" name="layoutName" class="form-control" value="<%=layoutData[i].layoutName%>" placeholder="Type Designation Here" id="example-text-input" autocomplete="off">
                                                                                    </div>
                                                                                </div>
                                                                                <div class="row">
                                                                                    <div class="col-sm-2">
                                                                                        <label for="example-text-input" class="col-form-label">Drawn</label>
                                                                                        <input type="text" name="drawnBy" class="form-control" value="<%=layoutData[i].drawnBy%>" placeholder="Type Initials Here" id="example-text-input" autocomplete="off">
                                                                                    </div>
                                                                                    <div class="col-sm-4">
                                                                                        <label for="example-text-input" class="col-form-label">Drawn Date</label>
                                                                                        <input type="date" name="drawnDate" class="form-control" value="<%=layoutData[i].drawnDate%>" placeholder="Type Date Here" id="example-text-input" autocomplete="off">
                                                                                    </div>
                                                                                    <div class="col-sm-2">
                                                                                        <label for="example-text-input" class="col-form-label">Checked</label>
                                                                                        <input type="text" name="checkedBy" class="form-control" value="<%=layoutData[i].checkedBy%>" placeholder="Type Initials Here" id="example-text-input" autocomplete="off">
                                                                                    </div>
                                                                                    <div class="col-sm-4">
                                                                                        <label for="example-text-input" class="col-form-label">Checked Date</label>
                                                                                        <input type="date" name="checkedDate" class="form-control" value="<%=layoutData[i].checkedDate%>" placeholder="Type Date Here" id="example-text-input" autocomplete="off">
                                                                                    </div>
                                                                                </div>
                                                                                <div class="row">
                                                                                    <div class="col-sm-12">
                                                                                        <label for="example-text-input" class="col-form-label">Rev Note</label>
                                                                                        <input type="text" name="revNote" class="form-control" value="" placeholder="Type Note Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                            <div class="modal-footer justify-content-center">
                                                                                <div class="w-50">
                                                                                    <input id="editLayoutModalButton" type="submit" value="Save" class="btn-block btn btn-outline-info waves-effect waves-light" />
                                                                                </div>
                                                                            </div>
                                                                        </form>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <!-- END EDIT CURRENT LAYOUT -->
                                                <% } %>
                                            <% } %>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-sm-6">
                                    <div class="card m-b-20" style="height:94%;">
                                        <div class="card-body">
                                            <h4 class="mt-0 header-title">Creo Data</h4>
                                            <% if (message != null) { %>
                                                <% if (message.location == 2) { %>
                                                    <h6 style="color:red;"><%= message.text %></h6>
                                                <% } %>
                                            <% } %>
                                            <% if  (creoData.length != 0) { %>
                                                <% for (let i = 0; i < creoData.length; i++) { %>
                                                    <form id="creoDataForm" class="creoDataForm" action="../layoutSetWD/?layoutID=<%= layoutData[0].jobNum%><%= layoutData[0].releaseNum%>_<%= layoutData[0].layoutID%>" method="POST">
                                                        <div class="form-group">
                                                            <div class="row">
                                                                <div class="col-sm-12">
                                                                    <label for="example-text-input" class="col-form-label">Layout Assembly</label>
                                                                    <input type="text" name="layoutAsm" class="form-control" value="<%=creoData[i].layoutAsm%>" placeholder="Type Layout Assembly Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="col-sm-12">
                                                                    <label for="example-text-input" class="col-form-label">Layout Drawing</label>
                                                                    <input type="text" name="layoutDrw" class="form-control" value="<%=creoData[i].layoutDrw%>" placeholder="Type Layout Drawing Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="col-sm-12">
                                                                    <label for="example-text-input" class="col-form-label">Working Directory</label>
                                                                    <input type="text" name="workingDir" class="form-control" value="<%=creoData[i].workingDir%>" placeholder="Type Working Directory Here" id="example-text-input" autocomplete="off">
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="col-sm-12">
                                                                    <label for="example-text-input" class="col-form-label">Standard Library</label>
                                                                    <input type="text" name="standardLib" class="form-control" value="<%=creoData[i].standardLib%>" placeholder="Type Standard Library Here" id="example-text-input" autocomplete="off">
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="form-group">
                                                            <div class="row justify-content-center">
                                                                <div class="col-4">
                                                                    <input id="setWDBtn" type="submit" value="Set Working Directory" class="btn-block btn btn-outline-secondary waves-effect waves-light" />
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </form>
                                                    <form id="generateLayoutForm" class="generateLayoutForm" action="../generateLayout/?layoutID=<%= layoutData[0].jobNum%><%= layoutData[0].releaseNum%>_<%= layoutData[0].layoutID%>" method="POST">
                                                        <div class="row justify-content-center">
                                                            <div class="col-4">
                                                                <input id="generateLayoutBtn" type="submit" value="Generate Layout" class="btn-block btn btn-outline-success waves-effect waves-light" />
                                                            </div>
                                                        </div>
                                                    </form>
                                                <% } %>
                                            <% } %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row justify-content-center">
                                <div class="col-sm-12">
                                    <div class="card m-b-20" style="height:94%;">
                                        <div class="card-body">
                                            <h4 class="mt-0 header-title">Layout Data</h4>
                                            <% if (layoutDetail.length == 0) { %>
                                            <form id="layoutPnForm" class="layoutPnForm" action="/reverseEngineerLayoutDetail/?layoutID=<%=layoutData[0].jobNum%><%=layoutData[0].releaseNum%>_<%=layoutData[0].layoutID%>" method="POST">
                                                <% if (reverseEngineerLayoutDetail.length == 0) { %>
                                                    <div class="row justify-content-center">
                                                        <div class="col-sm-4">
                                                            <div class="form-group">
                                                                <label for="example-text-input" class="col-form-label">Product Part Number</label>
                                                                <div class="input-group mb-3">
                                                                    <input type="text" name="layoutCatalogPN" class="form-control" placeholder="Type Part Number Here" aria-label="Type Part Number Here" aria-describedby="basic-addon2" autocomplete="off">
                                                                    <div class="input-group-append">
                                                                        <button class="btn btn-outline-secondary" type="submit">Reverse Engineer</button>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                <% } else { %>
                                                    <div class="row justify-content-center">
                                                        <div class="col-sm-4">
                                                            <div class="form-group">
                                                                <label for="example-text-input" class="col-form-label">Product Part Number</label>
                                                                <div class="input-group mb-3">
                                                                    <input type="text" name="layoutCatalogPN" value="<%= reverseEngineerLayoutDetail[0].layoutCatalogPN %>" class="form-control" placeholder="Type Part Number Here" aria-label="Type Part Number Here" aria-describedby="basic-addon2" autocomplete="off">
                                                                    <div class="input-group-append">
                                                                        <button class="btn btn-outline-secondary" type="submit">Reverse Engineer</button>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                <% } %>
                                            </form>
                                            <form id="layoutDetailForm" class="layoutDetailForm" action="/addLayoutDetail/?layoutID=<%=layoutData[0].jobNum%><%=layoutData[0].releaseNum%>_<%=layoutData[0].layoutID%>" method="POST">
                                                <% if (reverseEngineerLayoutDetail.length == 0) { %>
                                                    <div class="row">
                                                        <div class="col-sm-3">
                                                            <div class="form-group">
                                                                <label for="productFamily" class="col-form-label">Product Family</label>
                                                                <select class="form-control" name="productFamily" id="productFamily">
                                                                    <option value="">Select</option>
                                                                    <% for (let family of productPnOpts.family) { %>
                                                                        <option data-parent value="<%= family.description %>"><%= family.description %></option>
                                                                    <% } %>
                                                                </select>
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-3">
                                                            <div class="form-group">
                                                                <label for="productLine" class="col-form-label">Product Line</label>
                                                                <select class="form-control" name="productLine" id="productLine">
                                                                    <option value="">Select</option>
                                                                    <% for (let prodLine of productPnOpts.prodLine) { %>
                                                                        <option data-parent value="<%= prodLine.description %>"><%= prodLine.description %></option>
                                                                    <% } %>
                                                                </select>
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-2">
                                                            <div class="form-group">
                                                                <label for="systemVolt" class="col-form-label">System Voltage</label>
                                                                <select class="form-control" name="systemVolt" id="systemVolt">
                                                                    <option value="">Select</option>
                                                                    <% for (let systemVolt of productPnOpts.systemVolt) { %>
                                                                        <option data-parent value="<%= systemVolt.description %>"><%= systemVolt.description %></option>
                                                                    <% } %>
                                                                </select>
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-2">
                                                            <div class="form-group">
                                                                <label for="currentRating" class="col-form-label">Current Rating</label>
                                                                <select class="form-control" name="currentRating" id="currentRating">
                                                                    <option value="">Select</option>
                                                                    <% for (let currentRating of productPnOpts.currentRating) { %>
                                                                        <option data-parent value="<%= currentRating.description %>"><%= currentRating.description %></option>
                                                                    <% } %>
                                                                </select>
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-2">
                                                            <div class="form-group">
                                                                <label for="interruptingRating" class="col-form-label">Interrupting Rating</label>
                                                                <select class="form-control" name="interruptingRating" id="interruptingRating">
                                                                    <option value="">Select</option>
                                                                    <% for (let interruptingRating of productPnOpts.interruptingRating) { %>
                                                                        <option data-parent value="<%= interruptingRating.description %>"><%= interruptingRating.description %></option>
                                                                    <% } %>
                                                                </select>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-sm-3">
                                                            <div class="form-group">
                                                                <label for="enclosure" class="col-form-label">Enclosure</label>
                                                                <select class="form-control" name="enclosure" id="enclosure">
                                                                    <option value="">Select</option>
                                                                    <% for (let enclosure of productPnOpts.enclosure) { %>
                                                                        <option data-parent value="<%= enclosure.description %>"><%= enclosure.description %></option>
                                                                    <% } %>
                                                                </select>
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-3">
                                                            <div class="form-group">
                                                                <label for="finish" class="col-form-label">Finish</label>
                                                                <select class="form-control" name="finish" id="finish">
                                                                    <option value="">Select</option>
                                                                    <% for (let finish of productPnOpts.finish) { %>
                                                                        <option data-parent value="<%= finish.description %>"><%= finish.description %></option>
                                                                    <% } %>
                                                                </select>
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-3">
