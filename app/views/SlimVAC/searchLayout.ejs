                                                                <input type="date" name="drawnDate" class="form-control" value="<%=layoutData[i].drawnDate%>" placeholder="Type Date Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                            <div class="col-sm-2">
                                                                <label for="example-text-input" class="col-form-label">Checked</label>
                                                                <input type="text" name="checkedBy" class="form-control" value="<%=layoutData[i].checkedBy%>" placeholder="Type Initials Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                            <div class="col-sm-4">
                                                                <label for="example-text-input" class="col-form-label">Checked Date</label>
                                                                <input type="date" name="checkedDate" class="form-control" value="<%=layoutData[i].checkedDate%>" placeholder="Type Date Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                        </div>
                                                        <div class="row">
                                                            <div class="col-sm-12">
                                                                <label for="example-text-input" class="col-form-label">Rev Note</label>
                                                                <input type="text" name="revNote" class="form-control" value="" placeholder="Type Note Here" id="example-text-input" autocomplete="off" readonly="readonly">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </form>
                                                <!-- EDIT CURRENT LAYOUT -->
                                                <div class="row justify-content-center">
                                                    <div class="col-3">
                                                        <input id="editLayoutBtn" type="button" value="Edit" class="btn-block btn btn-outline-warning waves-effect waves-light" data-toggle="modal" data-target="#editLayoutModal" style="margin-bottom:10%;"/>
